    "@rspack/plugin-react-refresh": "1.0.0-alpha.1",
    "debug": "^4.3.4",
    "diff": "^5.1.0",
    "diff2html": "^3.4.45",
    "html-escaper": "^3.0.3",
    "lodash": "^4.17.21",
    "opener": "^1.5.2",
    "rspack-manifest-plugin": "^5.0.0",
    "rspack-plugin-virtual-module": "^0.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@rspack/cli": "1.0.0-alpha.1",
    "@types/debug": "^4.1.12",
    "@types/html-escaper": "^3.0.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.9.0",
    "@types/opener": "^1.4.3",
